<mapper namespace="com.workflow.demo.dao.FhlogMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.workflow.demo.entity.system.Fhlog">
		<id column="FHLOG_ID" property="fhlogId" />
		<result column="USERNAME" property="username" />
		<result column="CZTIME" property="cztime" />
		<result column="CONTENT" property="content" />
